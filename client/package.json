{
  "name": "fcc-react",
  "version": "1.0.0",
  "description": "A collection of projects for the Free Code Camp curriculum, written with React",
  "main": "src/server/index.js",
  "scripts": {
    "test": "jest --watchAll --coverage",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development --hot",
    "build": "webpack --mode production"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "moduleFileExtensions": [
      "css",
      "scss",
      "js",
      "json",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/src/__mocks__/styleMock.js",
      "\\.(jpg|gif|ttf|eot|svg)$": "<rootDir>/src/__mocks__/fileMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JimmayVV/fcc-react.git"
  },
  "keywords": [
    "react",
    "react-router"
  ],
  "author": "Jimmy Van Veen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JimmayVV/fcc-react/issues"
  },
  "homepage": "https://github.com/JimmayVV/fcc-react#readme",
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-react-app": "^3.0.4",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-css-modules-transform": "^2.1.1",
    "jest-dom": "^2.1.0",
    "jest-fetch-mock": "^1.6.6",
    "jsdom": "^12.2.0",
    "prettier": "^1.14.3",
    "react-hot-loader": "^4.3.11",
    "react-testing-library": "^5.2.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  }
}
